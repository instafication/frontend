# PostHog Analytics - Client-side analytics
# Format: String
# Example: "phc_HLDbWoUxDBzdI73yrjji2OWu6rLTzlVfThdLuh7wtYH"
PUBLIC_POSTHOG_KEY=""

# PostHog API Host - Endpoint for PostHog analytics
# Format: URL string
# Example: "https://eu.i.posthog.com"
PUBLIC_POSTHOG_HOST=""

# Database Connection - PostgreSQL connection string for Prisma ORM
# Format: postgresql://[user[:password]@][host][:port][/database][?parameters]
# Example: "postgresql://postgres:password@db.project.supabase.co:5432/postgres?sslmode=require"
PUBLIC_DATABASE_URL=""

# Direct Database Connection - Used by Prisma for migrations
# Format: postgresql://[user[:password]@][host][:port][/database][?parameters]
# Example: "postgresql://postgres:password@db.project.supabase.co:5432/postgres?sslmode=require"
PUBLIC_DIRECT_URL=""

# Supabase Project URL - Base URL for your Supabase project
# Format: https://[project-ref].supabase.co
# Example: "https://your-project-ref.supabase.co"
PUBLIC_SUPABASE_URL=""

# Supabase Anonymous Key - Public API key for Supabase client
# Format: JWT token or Supabase key format
# Example: "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9..." or "sb_project_anon_key"
PUBLIC_SUPABASE_ANON_KEY=""

# Stripe Restricted Key - Limited access key for client-side operations
# Format: rk_test_... or rk_live_...
# Example: "rk_test_51J1234567890"
STRIPE_SECRET_KEY=""

# Stripe Publishable Key - Public key for client-side Stripe operations
# Format: pk_test_... or pk_live_...
# Example: "pk_test_1234567890abcdef"
SECRET_STRIPE_PUBLISHABLE_KEY=""

# Stripe Secret Key - Full access key for server-side Stripe operations
# Format: sk_test_... or sk_live_...
# Example: "sk_test_1234567890abcdef"
SECRET_STRIPE_SECRET_KEY=""

# Stripe Webhook Secret - Used to verify webhook events from Stripe
# Format: whsec_...
# Example: "whsec_1234567890abcdef"
SECRET_STRIPE_WEBHOOK_SECRET=""