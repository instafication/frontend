{
  "name": "instafication",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "license": "MIT",
  "packageManager": "bun@1.2",
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "dev": "vite dev",
    "dev:wrangler": "drizzle-kit generate && vite build && wrangler dev --x-remote-bindings",
    "build": "vite build",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write .",
    "lint": "eslint . --ext .ts,.svelte && prettier --check .",
    "cf-typegen": "wrangler types ./src/worker-configuration.d.ts",
    "deploy": "bun run build && bun run cf-typegen && wrangler deploy",
    "generate": "drizzle-kit generate",
    "push": "drizzle-kit push",
    "migrate:prod": "drizzle-kit generate && wrangler d1 migrations apply prod --remote",
    "studio": "drizzle-kit studio",
    "logs": "wrangler tail frontend",
    "clean": "rm -rf .svelte-kit && rm -rf node_modules && rm -rf .wrangler && rm -rf .svelte-kit && rm -f bun.lock && rm -f package-lock.json && bun i"
  },
  "dependencies": {
    "@better-auth/utils": "^0.2.6",
    "@neondatabase/serverless": "^1.0.1",
    "better-auth": "^1.3.7",
    "better-auth-cloudflare": "^0.2.4",
    "clsx": "^2.1.1",
    "drizzle-orm": "^1.0.0-beta.1-fd5d1e8",
    "drizzle-valibot": "^0.4.2",
    "flowbite-svelte": "^1.12.6",
    "hash-wasm": "^4.1.0",
    "posthog-js": "^1.260.1",
    "resend": "^6.0.1",
    "stripe": "^18.4.0",
    "tailwind-merge": "^3.3.1",
    "tailwind-variants": "^2.1.0",
    "valibot": "^1.1.0"
  },
  "devDependencies": {
    "@eslint/compat": "^1.3.2",
    "@eslint/js": "^10.0.0",
    "@internationalized/date": "^3.8.2",
    "@lucide/svelte": "^0.540.0",
    "@sveltejs/adapter-cloudflare": "^7.2.1",
    "@sveltejs/kit": "^2.33.1",
    "@sveltejs/vite-plugin-svelte": "^6.1.3",
    "@tailwindcss/vite": "^4.1.12",
    "@types/pg": "^8.15.5",
    "better-sqlite3": "^12.2.0",
    "bits-ui": "^2.9.4",
    "drizzle-kit": "^1.0.0-beta.1-fd5d1e8",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-svelte": "^3.11.0",
    "flowbite": "^3.1.2",
    "globals": "^16.3.0",
    "mode-watcher": "^1.1.0",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "svelte": "^5.38.2",
    "svelte-check": "^4.3.1",
    "svelte-flag-icons": "^2.0.2",
    "svelte-sonner": "^1.0.5",
    "tailwindcss": "^4.1.12",
    "toml": "^3.0.0",
    "tw-animate-css": "^1.3.7",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.40.0",
    "vite": "^7.1.3",
    "wrangler": "^4.31.0",
    "zod": "^4.0.17"
  }
}