{
  "name": "instafication",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "cf-typegen": "wrangler types ./src/worker-configuration.d.ts",
    "deploy": "bun run build && bun run cf-typegen && wrangler deploy",
    "migrate:dev": "wrangler d1 migrations apply DB",
    "migrate:prod": "wrangler d1 migrations apply DB --remote",
    "generate": "drizzle-kit generate",
    "push": "drizzle-kit push",
    "clean": "rm -rf .svelte-kit && rm -rf node_modules && rm -rf .wrangler && bun i",
    "studio": "drizzle-kit studio"
  },
  "dependencies": {
    "@neondatabase/serverless": "^1.0.1",
    "@supabase/supabase-js": "^2.53.0",
    "@trpc/client": "^11.4.3",
    "@trpc/server": "^11.4.3",
    "classnames": "^2.5.1",
    "drizzle-orm": "^0.44.4",
    "flowbite": "^3.1.2",
    "flowbite-svelte": "^1.11.0",
    "posthog-js": "^1.258.5",
    "resend": "^4.7.0",
    "stripe": "^18.4.0",
    "superjson": "^2.2.2",
    "svelte-check": "^4.3.0",
    "typescript": "^5.9.2",
    "vite": "npm:rolldown-vite@latest",
    "wrangler": "^4.27.0",
    "zod": "^4.0.14"
  },
  "devDependencies": {
    "@internationalized/date": "^3.8.2",
    "@lucide/svelte": "^0.536.0",
    "@sveltejs/adapter-cloudflare": "^7.1.2",
    "@sveltejs/kit": "^2.27.0",
    "@tailwindcss/vite": "^4.1.11",
    "@types/pg": "^8.15.5",
    "bits-ui": "^2.9.0",
    "clsx": "^2.1.1",
    "drizzle-kit": "^0.31.4",
    "mode-watcher": "^1.1.0",
    "svelte": "^5.37.2",
    "svelte-flags": "^2.0.1",
    "svelte-sonner": "^1.0.5",
    "tailwind-merge": "^3.3.1",
    "tailwind-variants": "^2.1.0",
    "tailwindcss": "^4.1.11",
    "tw-animate-css": "^1.3.6"
  }
}