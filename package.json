{
	"name": "instafication",
	"private": true,
	"version": "0.1.0",
	"type": "module",
	"license": "MIT",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"deploy": "bun run build && wrangler types && wrangler pages deploy .svelte-kit/cloudflare",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"prepare": "svelte-kit sync || echo ''",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"clean": "rm -rf .svelte-kit && rm -rf node_modules && rm -rf .wrangler && bun i"
	},
	"dependencies": {
		"@cloudflare/workers-types": "^4.20250730.0",
		"@prisma/client": "^6.13.0",
		"@supabase/supabase-js": "^2.53.0",
		"@sveltejs/adapter-cloudflare": "^7.1.1",
		"@sveltejs/kit": "^2.26.1",
		"@tailwindcss/vite": "^4.1.11",
		"@trpc/client": "^11.4.3",
		"@trpc/server": "^11.4.3",
		"classnames": "^2.5.1",
		"flowbite": "^3.1.2",
		"flowbite-svelte": "^1.10.21",
		"stripe": "^18.3.0",
		"superjson": "^2.2.2",
		"svelte": "^5.37.1",
		"tailwindcss": "^4.1.11",
		"typescript": "^5.9.1-rc",
		"vite": "npm:rolldown-vite@latest",
		"wrangler": "^4.26.1",
		"zod": "^4.0.13"
	},
	"devDependencies": {
		"prisma": "^6.13.0",
		"svelte-flags": "^2.0.1"
	}
