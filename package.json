{
  "name": "instafication",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "license": "MIT",
  "packageManager": "bun@1.3",
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "dev": "vite dev",
    "dev:wrangler": "drizzle-kit generate && vite build && wrangler dev",
    "build": "vite build",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write .",
    "lint": "eslint . --ext .ts,.svelte && prettier --check .",
    "cf-typegen": "wrangler types ./src/worker-configuration.d.ts",
    "deploy": "bun run build && bun run cf-typegen && wrangler deploy",
    "generate": "drizzle-kit generate",
    "push": "drizzle-kit push",
    "migrate:prod": "drizzle-kit generate && wrangler d1 migrations apply prod --remote",
    "studio": "drizzle-kit studio",
    "logs": "wrangler tail frontend",
    "clean": "rm -rf .svelte-kit && rm -rf node_modules && rm -rf .wrangler && rm -rf .svelte-kit && rm -f bun.lock && rm -f package-lock.json && bun i"
  },
  "dependencies": {
    "@better-auth/utils": "^0.3.0",
    "@neondatabase/serverless": "^1.0.2",
    "better-auth": "^1.3.34",
    "better-auth-cloudflare": "^0.2.8",
    "clsx": "^2.1.1",
    "drizzle-orm": "^1.0.0-beta.1-fd8bfcc",
    "drizzle-valibot": "^0.4.2",
    "flowbite-svelte": "^1.26.0",
    "hash-wasm": "^4.12.0",
    "posthog-js": "^1.290.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "stripe": "^19.3.0",
    "tailwind-merge": "^3.4.0",
    "tailwind-variants": "^3.1.1",
    "valibot": "^1.1.0"
  },
  "devDependencies": {
    "@eslint/compat": "^1.4.1",
    "@eslint/js": "^9.39.1",
    "@internationalized/date": "^3.10.0",
    "@lucide/svelte": "^0.553.0",
    "@sveltejs/adapter-cloudflare": "^7.2.4",
    "@sveltejs/kit": "^2.48.4",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@tailwindcss/vite": "^4.1.17",
    "@types/pg": "^8.15.6",
    "better-sqlite3": "^12.4.1",
    "bits-ui": "^2.14.2",
    "drizzle-kit": "^1.0.0-beta.1-fd8bfcc",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-svelte": "^3.13.0",
    "flowbite": "^3.1.2",
    "globals": "^16.5.0",
    "mode-watcher": "^1.1.0",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "svelte": "^5.43.5",
    "svelte-check": "^4.3.3",
    "svelte-flag-icons": "^3.0.0",
    "svelte-sonner": "^1.0.6",
    "tailwindcss": "^4.1.17",
    "toml": "^3.0.0",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.3",
    "vite": "^7.2.2",
    "wrangler": "^4.46.0",
    "zod": "^4.1.12"
  }
}